<script type="text/javascript">
    $(document).ready(function(){
        $.ajaxSetup({cache: true});
        var device = 'desktop';
        if( /Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
             device='mobile';
        }
        
        var is_load_device = 0;
        function loadjs_device() {
            if(is_load_device==0) {
                is_load_device=1;
                $('script[type='+device+'js][src]').each(function(){
                    $(this).attr('type', 'text/javascript');
                    var url = $(this).attr('src');
                    $.getScript(url);
                });
                $('iframe.lazyload').each(function(){
                    var src = $(this).attr('data-src');
                    $(this).removeClass('lazyload').attr('src', src);
                });
                setTimeout(function(){
                    $("div[id^='zone']:empty").css('min-height',0);
                }, 500);
            }
        }
        $(window).scroll(function(){
            loadjs_device();
        });
        $(window).mousemove(function(){
            loadjs_device();
        });
        
        var link_audio = $('#next_audio').attr('data-link');
        setCookie('adnext', link_audio, '7');
        audio_on = getCookie('audio_auto');
        if(audio_on==1){
            $('.checked_box').attr("checked", "checked");
        }

        $('#loadmore_tmoi').on('click', onLoad_tm);

        function onLoad_tm() {
            page++;
            var milliseconds = new Date().getTime();
            $.ajax({
                url: "/?mod=home&act=loadmore_tinmoi",
                method: 'get',
                data: { 'category_id': category_id, 'page': page, 'v': milliseconds },
                context: $('.list-news-home-2'),
            }).done(function (data) {
                $(this).append(data);
                $('#loading').css('display', 'none');
            });
        }
        
        $('#radio-toggle, #radio-toggle2, #radio-togglemb').change(function () {
            cname = 'audio_auto';
            auto_on = getCookie(cname);
            var d = new Date();
            var time = d.getTime();
              if(auto_on == 1){
                setCookie(cname, null, '-365');
                setCookie('adnext', null, '-365');
              }
              else{
                setCookie(cname, '1', '365');
                setCookie('adnext', link_audio, '7');
              }    
        });
    });
    $(document).ready(function(){
        $('.content_tv').find('figure.expNoEdit.vd').each(function() {
            $(this).remove();
        })
        $('input[type=text]').keyup(function() {
             $(this).val(compoundUnicode($(this).val()));
        });
        
    });
    

</script>
